{
  "name": "nestjs-fastify-template",
  "version": "1.0.0",
  "description": "nestjs fastify server template",
  "license": "MIT",
  "author": "zhihao",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=development nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'"
  },
  "dependencies": {
    "@fastify/csrf-protection": "7.0.2",
    "@fastify/helmet": "13.0.1",
    "@fastify/static": "8.1.1",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cli": "11.0.4",
    "@nestjs/common": "11.0.10",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "11.0.10",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "11.0.10",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.0.4",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/node": "22.13.5",
    "axios": "^1.7.9",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "dayjs": "^1.11.13",
    "fastify": "^5.2.1",
    "ioredis": "^5.5.0",
    "mysql2": "^3.12.0",
    "nestjs-pino": "^4.3.1",
    "nodejs-snowflake": "^2.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "pino-roll": "^3.0.0",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "rxjs": "7.8.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/schematics": "11.0.1",
    "@nestjs/testing": "11.0.10",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.18",
    "@types/express": "5.0.0",
    "@types/jest": "^29.5.14",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "8.24.1",
    "@typescript-eslint/parser": "8.24.1",
    "eslint": "9.21.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-import": "2.31.0",
    "jest": "29.7.0",
    "pino-pretty": "^13.0.0",
    "prettier": "3.5.2",
    "run-script-webpack-plugin": "^0.2.2",
    "supertest": "7.0.0",
    "ts-jest": "29.2.6",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.3",
    "webpack": "^5.98.0",
    "webpack-node-externals": "^3.0.0"
  }
}