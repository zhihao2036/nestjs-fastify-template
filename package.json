{
  "name": "nestjs-fastify-template",
  "version": "1.0.0",
  "description": "nestjs fastify server template",
  "license": "UNLICENSED",
  "author": "",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=development nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'"
  },
  "dependencies": {
    "@fastify/csrf-protection": "6.4.1",
    "@fastify/helmet": "11.1.1",
    "@fastify/static": "7.0.4",
    "@nestjs/axios": "^3.1.2",
    "@nestjs/cli": "10.4.8",
    "@nestjs/common": "10.4.10",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "10.4.10",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-fastify": "10.4.10",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/swagger": "^8.0.7",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/typeorm": "^10.0.2",
    "@types/node": "22.10.0",
    "axios": "^1.7.8",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "fastify": "^5.1.0",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "rxjs": "7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/schematics": "10.2.3",
    "@nestjs/testing": "10.4.10",
    "@swc/cli": "^0.5.1",
    "@swc/core": "^1.9.3",
    "@types/express": "5.0.0",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "8.16.0",
    "@typescript-eslint/parser": "8.16.0",
    "eslint": "9.15.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "jest": "29.7.0",
    "prettier": "3.4.1",
    "run-script-webpack-plugin": "^0.2.0",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.2",
    "webpack": "^5.96.1",
    "webpack-node-externals": "^3.0.0"
  }
}